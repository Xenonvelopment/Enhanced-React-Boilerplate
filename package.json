{
  "name": "master-boilerplate",
  "version": "0.1.0",
  "description": "invoice scraping",
  "main": "index.js",
  "author": "firemind",
  "scripts": {
    "build": "concurrently 'npm run webpack' 'npm run serverless:package'",
    "deploy": "BUILD_STAGE=development bash setup-scripts/deploy.sh",
    "deploy:production": "BUILD_STAGE=production bash setup-scripts/deploy.sh",
    "deploy:staging": "BUILD_STAGE=staging bash setup-scripts/deploy.sh",
    "serverless": "serverless --config infrastructure/serverless/serverless.yml",
    "serverless:offline": "serverless offline --config infrastructure/serverless/serverless.yml",
    "start": "concurrently 'npm run webpack-dev-server' 'npm run test'",
    "test": "standard --fix --verbose | snazzy",
    "webpack": "webpack --config infrastructure/webpack/webpack.config.client.js",
    "webpack-dev-server": "webpack-dev-server --config infrastructure/webpack/webpack.config.client.js"
  },
  "dependencies": {
    "aws-serverless-express": "^3.3.6",
    "axios": "^0.19.0",
